{
  "name": "directus-extension-chat",
  "description": "Please enter a description for your extension",
  "icon": "extension",
  "version": "1.0.0",
  "type": "module",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-endpoint"
  ],
  "directus:extension": {
    "type": "endpoint",
    "path": "dist/index.js",
    "source": "src/index.ts",
    "host": "9.24.0"
  },
  "scripts": {
    "build": "directus-extension build",
    "validate": "tsc --noEmit",
    "dev": "directus-extension build -w --no-minify --sourcemap",
    "link": "directus-extension link"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "9.24",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@types/node-fetch": "^2.6.5",
    "@types/qrcode": "^1.5.1",
    "typescript": "4.9.5"
  },
  "volta": {
    "node": "16.20.0",
    "npm": "8.19.4"
  },
  "dependencies": {
    "moment": "^2.29.4",
    "node-fetch": "^3.3.2",
    "qrcode": "^1.5.3"
  }
}
